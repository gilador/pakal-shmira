const createExpoWebpackConfigAsync = require('@expo/webpack-config');

console.log('gilad!!)
module.exports = async function (env, argv) {
  const config = await createExpoWebpackConfigAsync(env, argv);

  config.devServer = {
    proxy: {
      '/api': 'http://127.0.0.1:8190'
    }
  }

  return config;
};